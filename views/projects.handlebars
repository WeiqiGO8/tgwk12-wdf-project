		<section class="projects-container">
			<h1>Projects and Works</h1>
			<ul>
				{{#each projects}}
					<li>
						{{#if (eq type "research")}}
              <span class="proj-research">
                {{else}}
                <span class="proj-teaching">
                  {{/if}}
                  {{year}}
                </span>
						{{name}}
						{{type}}
						{{desc}}
						{{year}}
						{{dev}}
						<img src="{{url}}" alt="{{url}}" />
					</li>
				{{/each}}

			</ul>
		</section>

	</body>
</html>